<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>


<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="edu.westga.cs1302.weather.view.WeatherCodeBehind">
   <children>
      <Pane fx:id="thePane" prefHeight="500.0" prefWidth="400.0">
         <children>
            <MenuBar layoutY="2.0" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="400.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem fx:id="buttonOpenFile" mnemonicParsing="false" onAction="#chooseFile" text="Open" />
                        <MenuItem fx:id="buttonExit" mnemonicParsing="false" text="Exit" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem fx:id="buttonAbout" mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </Pane>
   </children>
</AnchorPane>
